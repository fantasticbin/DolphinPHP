{
    "name": "dolphinphp/dolphinphp",
    "description": "DolphinPHP - A fast development framework powered by Laravel",
    "type": "project",
    "keywords": [
        "framework",
        "laravel",
        "dolphinphp"
    ],
    "homepage": "http://www.dolphinphp.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "DolphinPHP Team",
            "email": "admin@dolphinphp.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "ezyang/htmlpurifier": "^4.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "laravel-app/app/",
            "Database\\Factories\\": "laravel-app/database/factories/",
            "Database\\Seeders\\": "laravel-app/database/seeders/"
        },
        "files": [
            "laravel-app/app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "laravel-app/tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [],
        "post-update-cmd": [],
        "post-root-package-install": [],
        "post-create-project-cmd": []
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
